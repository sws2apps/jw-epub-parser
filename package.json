{
  "name": "jw-epub-parser",
  "version": "1.3.1",
  "type": "module",
  "description": "This tool will help you to parse and extract the needed source materials from Meeting Workbook EPUB file. Support for parsing Watchtower Study will be added in future release.",
  "keywords": [
    "epub parse",
    "jw",
    "jw.org",
    "epub"
  ],
  "source": "src/index.js",
  "exports": {
    "require": "./dist/index.cjs",
    "default": "./dist/index.modern.js"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.module.js",
  "unpkg": "./dist/index.umd.js",
  "files": [
    "dist/*.js",
    "dist/*.cjs"
  ],
  "homepage": "https://github.com/sws2apps/jw-epub-parser#readme",
  "author": "Scheduling Workbox System <sws2apps@gmail.com>",
  "bugs": {
    "url": "https://github.com/sws2apps/jw-epub-parser/issues",
    "email": "sws2apps@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sws2apps/jw-epub-parser.git"
  },
  "scripts": {
    "semantic-release": "semantic-release",
    "preghcommit": "git add .",
    "ghcommit": "git-cz",
    "test": "mocha",
    "build": "microbundle --no-sourcemaps"
  },
  "dependencies": {
    "@gxl/epub-parser": "^2.0.4",
    "jsdom": "^19.0.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "chai": "^4.3.6",
    "cz-conventional-changelog": "^3.3.0",
    "microbundle": "^0.14.2",
    "mocha": "^9.2.0",
    "semantic-release": "^19.0.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
